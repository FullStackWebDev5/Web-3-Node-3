<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title><%=firstName%>'s details</title>
	<link rel="stylesheet" href="../index.css">
</head>
<body>
	<div id="container">
		<h1><%=firstName%></h1>
		<img src="<%=image ? image : 'https://innostudio.de/fileuploader/images/default-avatar.png' %>" width="100" height="100">
		<ul>
			<li>Batch: <%=batchNo%></li>
			<li>Course: <%=techStack%> Stack</li>
			<li>Country: <%=country%></li>
		</ul>
		<h3>Hobbies</h3>
		<ul>
			<%hobbies.forEach((hobby) => {%>
				<li><%=hobby%></li>
			<%})%>
		</ul>
		<%if(experience){%>
			<h3>Experience</h3>
			<p><%=experience%></p>
		<%}%>
	</div>
</body>
</html>